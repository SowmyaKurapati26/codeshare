{
    "name": "codeshare",
    "version": "1.0.0",
    "description": "Real-time collaborative code editor",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install",
        "start": "cd server && npm start"
    },
    "keywords": [
        "collaborative",
        "code-editor",
        "real-time",
        "react",
        "nodejs",
        "socketio",
        "monaco-editor"
    ],
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/codeshare.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/codeshare/issues"
    },
    "homepage": "https://github.com/yourusername/codeshare#readme"
}